@model Daily_Use_App.Models.MoodEntry

<h2>Mood Entry Details</h2>

<div>
    <strong>Score:</strong> @Model.Score <br />
    <strong>Note:</strong> @Model.Note <br />
    <strong>Checked At:</strong> @Model.CheckedAt.ToShortDateString() <br />
    <strong>User:</strong> @Model.User?.Id <br />
</div>

<a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-warning">Edit</a>
<a asp-action="Index" class="btn btn-secondary">Back</a>
